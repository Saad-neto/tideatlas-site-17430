# API de Marés - Configuração
# Escolha um provedor: 'mock', 'stormglass' ou 'worldtides'
VITE_TIDE_PROVIDER=mock

# Stormglass.io API (50 requests/dia grátis)
# Obtenha sua chave em: https://stormglass.io/
VITE_STORMGLASS_API_KEY=

# WorldTides API ($0.01/request)
# Obtenha sua chave em: https://www.worldtides.info/
VITE_WORLDTIDES_API_KEY=

# Duração do cache em minutos (padrão: 60)
VITE_CACHE_DURATION=60

# Google AdSense (para monetização)
VITE_ADSENSE_CLIENT_ID=

# Google Analytics 4
VITE_GA4_MEASUREMENT_ID=
